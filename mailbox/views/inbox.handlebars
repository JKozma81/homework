<h2>Messages in your box:</h2>

<table>
    <tr>
        <th>Date</th>
        <th>From</th>
        <th>Subject</th>
    </tr>
    {{#each inboxMsgs}}
        <tr>
            <td>{{date}}</td>
            <td>{{from}}</td>
            <td>{{subject}}</td>
            <td><a href="/view/{{id}}">Read</a></td>
        </tr>
    {{/each}}
</table>